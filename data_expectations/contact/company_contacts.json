{
	"expectation_suite_name": "contact_company_contacts",
	"expectations": [
		{
			"expectation_type": "expect_column_values_to_not_be_null",
			"kwargs": { "column": "Company" }
		},
		{
			"expectation_type": "expect_column_values_to_match_regex",
			"kwargs": {
				"column": "Email",
				"regex": "^[^@\\s]+@[^@\\s]+\\.[^@\\s]+$",
				"mostly": 0.8
			}
		},
		{
			"expectation_type": "expect_column_values_to_match_regex",
			"kwargs": {
				"column": "Cellnumber",
				"regex": "^\\+?[0-9\\s-]{6,}$",
				"mostly": 0.8
			}
		},
		{
			"expectation_type": "expect_column_values_to_be_in_set",
			"kwargs": {
				"column": "EmailValidationStatus",
				"value_set": ["deliverable", "risky", "undeliverable", "unknown"],
				"mostly": 0.9
			}
		},
		{
			"expectation_type": "expect_column_values_to_be_between",
			"kwargs": {
				"column": "EmailValidationConfidence",
				"min_value": 0.0,
				"max_value": 1.0,
				"mostly": 1.0
			}
		},
		{
			"expectation_type": "expect_column_values_to_be_in_set",
			"kwargs": {
				"column": "PhoneValidationStatus",
				"value_set": ["deliverable", "risky", "undeliverable", "unknown"],
				"mostly": 0.9
			}
		},
		{
			"expectation_type": "expect_column_values_to_be_between",
			"kwargs": {
				"column": "PhoneValidationConfidence",
				"min_value": 0.0,
				"max_value": 1.0,
				"mostly": 1.0
			}
		}
	],
	"meta": {
		"notes": ["Contact roster must keep traceable communication fields"]
	}
}
